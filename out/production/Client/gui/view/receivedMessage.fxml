<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<fx:root minHeight="-Infinity" type="VBox" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <Hyperlink fx:id="usernameLink" maxHeight="1.7976931348623157E308" wrapText="true">
            <font>
                <Font name="System Bold" size="13.0"/>
            </font>
            <padding>
                <Insets left="-1.0"/>
            </padding>
        </Hyperlink>
        <Label fx:id="messageLabel" alignment="TOP_LEFT" wrapText="true">
            <font>
                <Font name="Arial" size="14.0"/>
            </font>
        </Label>
    </children>
    <padding>
        <Insets bottom="5.0" left="15.0" right="15.0" top="5.0"/>
    </padding>
</fx:root>
